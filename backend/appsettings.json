{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://localhost:8169"
            }
        }
    },
    "Uploads": "uploads",
    "ConnectionStrings": {
        "DefaultConnection": "Data Source=SquadFile.db"
    },
    "Jwt": {
        "Key": "SquadFileSecretKey1234567890123456",
        "Issuer": "SquadFileIssuer",
        "Audience": "SquadFileAudience",
        "ExpireDays": 3
    }
}